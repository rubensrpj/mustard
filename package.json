{
  "name": "mustard-claude",
  "version": "2.0.5",
  "description": "Framework-agnostic CLI for Claude Code project setup",
  "type": "module",
  "bin": {
    "mustard": "./bin/mustard.js"
  },
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "files": [
    "bin/",
    "dist/",
    "templates/"
  ],
  "scripts": {
    "start": "node bin/mustard.js",
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "node --test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "ai",
    "cli",
    "scaffold"
  ],
  "author": "Competi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Competi/mustard.git"
  },
  "homepage": "https://github.com/Competi/mustard#readme",
  "bugs": {
    "url": "https://github.com/Competi/mustard/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^13.0.1",
    "inquirer": "^9.2.15",
    "ollama": "^0.5.11",
    "ora": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.2.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  }
}
